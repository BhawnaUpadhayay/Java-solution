import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Enter number:");
        int a=input.nextInt();
        String vote=vote(a);
        System.out.println(vote);
    }
        public static String vote(int a){
            if(a>=18){
               return "Eligible";
        }
            else{
               return "Not eligible";
        }
}
}
