import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("enter three numbers :");
        int a=input.nextInt();
        int b=input.nextInt();
        int c=input.nextInt();
        int smaller=smaller(a,b,c);
        int larger=larger(a,b,c);
        System.out.println("The larger number is : "+larger);
        System.out.print("The smallest number is : "+smaller);
        input.close();
    }
public static int larger(int a,int b, int c){
    int max=a;
    if (b > max){
        max=b;
    }
    if(c > max){
        max=c;
    }
    return max;
}
public static int smaller(int a,int b,int c){
    int min=a;
    if (b < min){
        min=b;
    }
    if(c < min){
        min=c;
    }
    return min;
}
}


