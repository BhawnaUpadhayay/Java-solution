import java.util.Scanner;
class Name {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Enter two numbers");
        int a = input.nextInt();
        int b = input.nextInt();
        if(a>0)
        {
          System.out.println(a+b);
          System.out.println(a-b);
          System.out.println(a*b);
          System.out.println(a/b);
        }
        else{
          System.out.println("No output");
        }
        
    
    }
}
