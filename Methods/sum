import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Enter numbers:");
        int a=input.nextInt();
        int b=input.nextInt();
        int sum=sum(a,b);
        System.out.println(sum);
    }
        public static int sum(int a,int b){
            int ans=a+b;
            return ans;
}
}
