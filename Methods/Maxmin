import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Enter numbers:");
        int a=input.nextInt();
        int b=input.nextInt();
        int c=input.nextInt();
        int smaller=Smaller(a,b,c);
        System.out.println("The smaller number is "+smaller);
        int larger=Larger(a,b,c);
        System.out.print("The lareger number is "+larger);
    }
        public static int Smaller(int a,int b,int c){
            int min=a;
            if(b<min){
                min=b;
            }
            if(c<min){
                min=c;
            }
          return min;            
}
        public static int Larger(int a,int b,int c){
            int max=a;
            if(b>max){
                max=b;
            }
            if(c>max){
                max=c;
            }
          return max;            
}
}
